meta {
  name: Get all Activities (Unauthorized)
  type: http
  seq: 7
}

get {
  url: {{baseUrl}}/travel-guides/:id/activities
  body: none
  auth: none
}

params:path {
  id: {{id}}
}

headers {
  content-type: application/json
  x-tg-secret: invalid-secret
}

vars:post-response {
  actId: res.body[0].id
}

assert {
  res.status: eq 401
}
